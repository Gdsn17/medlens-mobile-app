{
  "name": "medlens-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "EXPO_NO_DOTENV=1 METRO_WATCHER_TYPE=none EXPO_NO_WATCHMAN=1 expo start",
    "start-ios": "EXPO_NO_DOTENV=1 METRO_WATCHER_TYPE=none EXPO_NO_WATCHMAN=1 expo start --ios",
    "start-android": "EXPO_NO_DOTENV=1 METRO_WATCHER_TYPE=none EXPO_NO_WATCHMAN=1 expo start --android",
    "start-tunnel": "EXPO_NO_DOTENV=1 METRO_WATCHER_TYPE=none EXPO_NO_WATCHMAN=1 expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev:phone": "expo start --tunnel",
    "dev:web": "expo start --web",
    "dev:ios": "expo start --ios",
    "dev:android": "expo start --android",
    "dev:fast": "expo start"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "expo": "~51.0.0",
    "expo-camera": "~15.0.16",
    "expo-image-picker": "~15.1.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.1",
    "firebase": "^10.14.1",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.12",
    "tailwindcss": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@types/react": "~18.2.14",
